{
    "liveSassCompile.settings.forceBaseDirectory": "/app/resources/assets",
    "liveSassCompile.settings.formats": [
        {
            "format": "expanded",
            "extensionName": ".css",
            "savePath": "~/../static/css",
            "savePathReplacementPairs": null
        }
    ],
    "cSpell.words": [
        "sitetitle",
        "wordmark"
    ],
    "files.exclude": {
        "**/__pycache__": true,
        "**/.idea": true
    }
}